{
  "name": "album-tracker-backend",
  "version": "1.0.0",
  "description": "Album Tracker Backend API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "db:init": "node scripts/init-db.js"
  },
  "keywords": [
    "album",
    "tracker",
    "musicbrainz"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "better-sqlite3": "^12.4.6",
    "fuse.js": "^7.1.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/express": "^5.0.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@eslint/js": "^9.39.1",
    "typescript-eslint": "^8.48.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.1",
    "typescript": "^5.9.3",
    "tsx": "^4.20.6",
    "vitest": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14"
  }
}
